{
    "name": "grapple-desktop",
    "version": "0.0.0",
    "author:": "",
    "description": "Electron React Template",
    "main": "build/app/main.js",
    "scripts": {
        "start": "grunt & electron .",
        "pack": "grunt & electron-builder --dir -w --x64",
        "watch": "nodemon --exec \"npm start\" ."
    },
    "dependencies": {
        "express": "^4.15.3",
        "firebase": "^4.1.3",
        "jquery": "^3.2.1",
        "react": "^15.3.2",
        "react-dom": "^15.3.2",
        "react-redux": "^5.0.5",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6"
    },
    "devDependencies": {
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "electron": "^1.6.11",
        "electron-builder": "^19.16.0",
        "grunt": "^1.0.1",
        "grunt-babel": "^6.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-sass": "^1.0.0",
        "nodemon": "^1.11.0"
    },
    "babel": {
        "plugins": [
            "transform-react-jsx"
        ],
        "presets": [
            "es2015"
        ]
    },
    "build": {
        "files": [
            "build/app/**/**.*"
        ]
    }
}
